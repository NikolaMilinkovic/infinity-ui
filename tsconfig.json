{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["es6"],
    "module": "node16",
    "moduleResolution": "node16",
    "noEmit": true,
    "strict": true,
    "target": "es2017",
    "types": ["react", "react-native"]
  },
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
