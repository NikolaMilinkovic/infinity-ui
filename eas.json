{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "simulator": false
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_BACKEND_URI": "https://infinity-server-b8ecd56d9909.herokuapp.com"
      }
    },
    "production": {
      "android": {
        "cache": { "disabled": true },
        "gradleCommand": ":app:assembleRelease",
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_BACKEND_URI": "https://infinity-server-b8ecd56d9909.herokuapp.com",
        "EAS_BUILD_DISABLE_NPM_CACHE": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
